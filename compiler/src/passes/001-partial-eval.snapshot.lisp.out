>> (program () 8)
=> (program () 8)
>> (program () (ineg 8))
=> (program () -8)
>> (program () (let ((x 8)) (iadd x x)))
=> (program () 16)
>> (program () x)
=> (program () x)
>> (program () (iadd x x))
=> (program () (iadd x x))
>> (program () (iadd (ineg 8) x))
=> (program () (iadd -8 x))
>> (program () (iadd (iadd 8 8) (ineg x)))
=> (program () (iadd 16 (ineg x)))
