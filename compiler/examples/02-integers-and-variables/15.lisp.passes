program:

  (program () (let ((x (let ((x 4)) (iadd x 1)))) (iadd x 2)))

uniquify:

  (program () (let ((x₁ (let ((x₂ 4)) (iadd x₂ 1)))) (iadd x₁ 2)))

remove-complex-operands:

  (program () (let ((x₁ (let ((x₂ 4)) (iadd x₂ 1)))) (iadd x₁ 2)))

explicate-control:

  (c-program () (:start ((= x₂ 4) (= x₁ (iadd x₂ 1)) (return (iadd x₁ 2)))))

select-instructions:

  [#cons-x86-program [:context [:x₂ #int-t :x₁ #int-t]] [:start [#cons-block [] [['movq [[#imm-rand 4] [#var-rand 'x₂]]] ['movq [[#var-rand 'x₂] [#var-rand 'x₁]]] ['addq [[#imm-rand 1] [#var-rand 'x₁]]] ['movq [[#var-rand 'x₁] [#reg-rand 'rax]]] ['addq [[#imm-rand 2] [#reg-rand 'rax]]] [#jmp 'epilog]]]]]

