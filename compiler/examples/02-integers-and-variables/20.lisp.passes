mod:

  (mod () (let ((x 1)) (iadd x (let ((x (let ((y 5)) (iadd y x)))) (iadd x 100)))))

shrink:

  (mod () (let ((x 1)) (iadd x (let ((x (let ((y 5)) (iadd y x)))) (iadd x 100)))))

uniquify:

  (mod () (let ((x₁ 1)) (iadd x₁ (let ((x₂ (let ((y₁ 5)) (iadd y₁ x₁)))) (iadd x₂ 100)))))

check-type:

  (mod () (the int-t (let ((x₁ (the int-t 1))) (the int-t (iadd (the int-t x₁) (the int-t (let ((x₂ (the int-t (let ((y₁ (the int-t 5))) (the int-t (iadd (the int-t y₁) (the int-t x₁))))))) (the int-t (iadd (the int-t x₂) (the int-t 100))))))))))

remove-complex-operands:

  (mod () (the int-t (let ((x₁ (the int-t 1))) (the int-t (let ((x₂ (the int-t (let ((y₁ (the int-t 5))) (the int-t (iadd (the int-t y₁) (the int-t x₁))))))) (the int-t (let ((_₁ (the int-t (iadd (the int-t x₂) (the int-t 100))))) (the int-t (iadd (the int-t x₁) (the int-t _₁))))))))))

explicate-control:

  [#cons-c-mod
   []
   [:begin
    [#cons-seq
     [#assign-stmt [#var-c-exp 'x₁] #int-type [#int-c-exp 1]]
     [#cons-seq
      [#assign-stmt [#var-c-exp 'y₁] #int-type [#int-c-exp 5]]
      [#cons-seq
       [#assign-stmt
        [#var-c-exp 'x₂]
        #int-type
        [#prim-c-exp 'iadd [[#var-c-exp 'y₁] [#var-c-exp 'x₁]]]]
       [#cons-seq
        [#assign-stmt
         [#var-c-exp '_₁]
         #int-type
         [#prim-c-exp 'iadd [[#var-c-exp 'x₂] [#int-c-exp 100]]]]
        [#return-seq [#prim-c-exp 'iadd [[#var-c-exp 'x₁] [#var-c-exp '_₁]]]]]]]]]]

uncover-locals:

  [#cons-c-mod
   [:locals [:x₁ #int-type :y₁ #int-type :x₂ #int-type :_₁ #int-type]]
   [:begin
    [#cons-seq
     [#assign-stmt [#var-c-exp 'x₁] #int-type [#int-c-exp 1]]
     [#cons-seq
      [#assign-stmt [#var-c-exp 'y₁] #int-type [#int-c-exp 5]]
      [#cons-seq
       [#assign-stmt
        [#var-c-exp 'x₂]
        #int-type
        [#prim-c-exp 'iadd [[#var-c-exp 'y₁] [#var-c-exp 'x₁]]]]
       [#cons-seq
        [#assign-stmt
         [#var-c-exp '_₁]
         #int-type
         [#prim-c-exp 'iadd [[#var-c-exp 'x₂] [#int-c-exp 100]]]]
        [#return-seq [#prim-c-exp 'iadd [[#var-c-exp 'x₁] [#var-c-exp '_₁]]]]]]]]]]

select-instructions:

  [#cons-x86-mod
   [:locals [:x₁ #int-type :y₁ #int-type :x₂ #int-type :_₁ #int-type]]
   [:begin
    [#cons-block
     []
     [['movq [[#imm-rand 1] [#var-rand 'x₁]]]
      ['movq [[#imm-rand 5] [#var-rand 'y₁]]]
      ['movq [[#var-rand 'y₁] [#var-rand 'x₂]]]
      ['addq [[#var-rand 'x₁] [#var-rand 'x₂]]]
      ['movq [[#var-rand 'x₂] [#var-rand '_₁]]]
      ['addq [[#imm-rand 100] [#var-rand '_₁]]]
      ['movq [[#var-rand 'x₁] [#reg-rand 'rax]]]
      ['addq [[#var-rand '_₁] [#reg-rand 'rax]]]
      [#jmp 'begin.epilog]]]]]

