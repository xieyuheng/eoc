program:

  (program () (iadd (iadd 1 2) (iadd 3 4)))

uniquify:

  (program () (iadd (iadd 1 2) (iadd 3 4)))

remove-complex-operands:

  (program () (let ((_₁ (iadd 1 2))) (let ((_₂ (iadd 3 4))) (iadd _₁ _₂))))

explicate-control:

  (c-program () (:begin ((= _₁ (iadd 1 2)) (= _₂ (iadd 3 4)) (return (iadd _₁ _₂)))))

select-instructions:

  [#cons-x86-program [:context [:_₁ #int-t :_₂ #int-t]] [:begin [#cons-block [] [['movq [[#imm-rand 1] [#var-rand '_₁]]] ['addq [[#imm-rand 2] [#var-rand '_₁]]] ['movq [[#imm-rand 3] [#var-rand '_₂]]] ['addq [[#imm-rand 4] [#var-rand '_₂]]] ['movq [[#var-rand '_₁] [#reg-rand 'rax]]] ['addq [[#var-rand '_₂] [#reg-rand 'rax]]] [#jmp 'begin.epilog]]]]]

